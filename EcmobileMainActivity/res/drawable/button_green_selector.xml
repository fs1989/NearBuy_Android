<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0px" android:color="#c97a7a7a" />
            <gradient android:startColor="#ff7f7f7f" android:endColor="#ff8e8e8e" android:angle="90.0" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <stroke android:width="1.0px" android:color="#c97a7a7a" />
            <gradient android:startColor="#ff7f7f7f" android:endColor="#ff8e8e8e" android:angle="90.0" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0px" android:color="#c9999999" />
            <gradient android:startColor="#669900" android:endColor="#669900" android:angle="90.0" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
</selector>